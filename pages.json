{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "首页"
			}
		}
	    ,{
            "path" : "pages/index/update",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "海报设计",
                "enablePullDownRefresh": false
            }
            
        },{
            "path" : "pages/index/my",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "我的",
                "enablePullDownRefresh": false
            }
            
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
	  "selectedColor": "#f94d2a",
	  "list": [
	    {
	      "pagePath": "pages/index/index",
	      "text": "推荐",
	      "iconPath": "static/tab-icons/hot.png",
	      "selectedIconPath": "static/tab-icons/hot-active.png"
	    },
	    {
	      "pagePath": "pages/index/my",
	      "text": "我的",
	      "iconPath": "static/tab-icons/my.png",
	      "selectedIconPath": "static/tab-icons/my-active.png"
	    }
	  ]
	},
	"easycom": {
	  "autoscan": true,
	  "custom": {
			"dialog-(.*)":"@/components/dialog/$1.vue",
			"sub-(.*)":"@/components/sub/$1.vue"
	  }
	}
}
